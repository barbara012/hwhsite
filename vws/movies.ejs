<html>
  <head>
    <meta charset="utf-8">
    <title>电影-<%= blog.title %></title>
    <link rel="stylesheet" href="/dist/css/app.css">
  </head>
  <body>
    <div class="app app-movies">
      <%- include('components/nav') %>
      <div class="app-main clearfix">
        <div class="app-main_content movies-list">
          <% movies.forEach(function (movie) { %>
            <%- include('components/movie-item', { movie: movie }) %>
          <% }) %>
          <%- include('components/page-nav') %>
        </div>
        <div class="app-main_side">
        </div>
      </div>
      <%- include('components/footer') %>
    </div>
  </body>
</html>
