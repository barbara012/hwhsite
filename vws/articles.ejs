<html>
  <head>
    <meta charset="utf-8">
    <title><%= blog.title %></title>
    <link rel="stylesheet" href="/dist/css/app.css">
  </head>
  <body>
    <div class="app app-news">
      <%- include('components/nav') %>
      <div class="app-main clearfix">
        <div class="app-banner">
          <img src="/jsimg/34ee15c7eb35a2b6.jpg" alt="">
        </div>
        <div class="app-main_content news-list">
          <% articles.forEach(function (article) { %>
            <%- include('components/article-item', { article: article }) %>
          <% }) %>
          <%- include('components/page-nav') %>
        </div>
        <%- include('components/side') %>
      </div>
      <%- include('components/footer') %>
    </div>
    <script>
      $.getScript('http://php.weather.sina.com.cn/iframe/index/w_cl.php?code=js&day=0&city=&dfc=1&charset=utf-8',function(a){
        var s=""
        var r=""
        var q=""
        for(s in window.SWther.w){
          q = SWther.w[s][0]
          r = {
            city: s,
            date: SWther.add.now.split(" ")[0]||"",
            day_weather: q.s1,
            night_weather: q.s2,
            day_temp: q.t1,
            night_temp: q.t2,
            day_wind: q.p1,
            night_wind: q.p2
          }
          console.log(r)
        }
      })
    </script>
  </body>
</html>
